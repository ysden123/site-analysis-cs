<Window x:Class="SiteAnalysisCS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SiteAnalysisCS"
        mc:Ignorable="d"
        Title="Site Analysis CS" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="Actions">
                <MenuItem Header="News startpage" Click="NewsStartPageMenuItemClick"/>
                <MenuItem Header="Knopka" Click="KnopkaMenuItemClick"/>
                <MenuItem Header="Isrageo" Click="IsrageoMenuItemClick"/>
                <MenuItem Header="Radio Svoboda" Click="RadioSvobodaMenuItemClick"/>
            </MenuItem>
        </Menu>
        <ContentControl x:Name="CC"/>
    </DockPanel>
</Window>
